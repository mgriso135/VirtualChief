<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="RitardiSoap" />
                <binding name="WarningSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://kis.localhost/Eventi/ritardi.asmx"
                binding="basicHttpBinding" bindingConfiguration="RitardiSoap"
                contract="KISScheduler.RitardiSoap" name="RitardiSoap" />
            <endpoint address="http://kis.localhost/Eventi/warning.asmx"
                binding="basicHttpBinding" bindingConfiguration="WarningSoap"
                contract="KISSchedulerWarning.WarningSoap" name="WarningSoap" />
        </client>
    </system.serviceModel>
</configuration>
