@model IEnumerable<KIS.App_Sources.InputPoint>
@{
    Layout = null;
}

@if(Model!=null && ViewBag.authX)
            {
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var m in Model)
                        {
                        <tr>
                            <td>
                                <a href="ListWorkstationsTasks2?InputPointId=@m.id">
                                    <span class="material-icons">
                                        login
                                    </span>
                                </a>
                            </td>
                            <td>@m.name</td>
                            <td>@m.description</td>
                        </tr>
                        }
                    </tbody>
                </table>
}