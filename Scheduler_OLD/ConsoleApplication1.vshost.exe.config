<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="RitardiSoap" />
                <binding name="WarningSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://kis.localhost/Eventi/Ritardi.asmx" binding="basicHttpBinding"
                bindingConfiguration="RitardiSoap" contract="KISScheduler.RitardiSoap"
                name="RitardiSoap" />
            <endpoint address="http://kis.localhost/Eventi/Warning.asmx" binding="basicHttpBinding"
                bindingConfiguration="WarningSoap" contract="KISSchedulerWarning.WarningSoap"
                name="WarningSoap" />
        </client>
    </system.serviceModel>
</configuration>